      "execution_count": 17,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Initialize dictionaries to store the transformed X and Y\n",
        "X_transformed_dict = {}\n",
        "Y_transformed_dict = {}\n",
        "\n",
        "\n",
        "# Loop over the items using the same index\n",
        "for i in range(7, 9):  # or any range based on your subjects\n",
        "    X_key = f'X_{i}'\n",
        "    Y_key = f'Y_{i}'\n",
        "    \n",
        "    if X_key in X_subjects_dict and Y_key in Y_subjects_dict:\n",
        "        X = X_subjects_dict[X_key]\n",
        "        Y = Y_subjects_dict[Y_key]\n",
        "        \n",
        "        # Apply the transform_for_classificator function\n",
        "        X_transformed, Y_transformed = transform_for_classificator(X, Y, Classes, Conditions)\n",
        "        \n",
        "        # Store the transformed values in new dictionaries\n",
        "        X_transformed_dict[X_key] = X_transformed\n",
        "        Y_transformed_dict[Y_key] = Y_transformed\n",
        "    else:\n",
        "        print(f\"Missing data for subject {i}: X_key: {X_key}, Y_key: {Y_key}\")"
      ]
