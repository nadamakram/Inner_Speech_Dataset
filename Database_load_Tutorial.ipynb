    {
      "cell_type": "code",
      "execution_count": 4,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Conditions to compared\n",
        "Conditions = [[\"Inner\"],[\"Inner\"],[\"Inner\"],[\"Inner\"]]\n",
        "# The class for the above condition\n",
        "Classes  = [ [\"Up\"] ,[\"Down\"],[\"Right\"],[\"Left\"]]"
